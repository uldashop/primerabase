<odoo>
    <data noupdate="1">
        <record id="rule_cat_ING" model="hr.salary.rule.category">
            <field name="code">ING</field>
            <field name="name">Ingresos</field>
        </record>

        <record id="rule_cat_EGRE" model="hr.salary.rule.category">
            <field name="code">EGRE</field>
            <field name="name">Egresos</field>
        </record>

        <record id="rule_cat_APTES" model="hr.salary.rule.category">
            <field name="code">APTE</field>
            <field name="name">Aportes</field>
            <field name="parent_id" ref="rule_cat_EGRE" />
        </record>

        <record id="rule_cat_PROV" model="hr.salary.rule.category">
            <field name="code">PROV</field>
            <field name="name">Provisiones</field>
        </record>

        <record id="rule_cat_NOAPOR" model="hr.salary.rule.category">
            <field name="code">NAPOR</field>
            <field name="name">No Aportable</field>
            <field name="parent_id" ref="rule_cat_ING" />
        </record>

        <record id="rule_cat_APOR" model="hr.salary.rule.category">
            <field name="code">APOR</field>
            <field name="name">Aportable</field>
            <field name="parent_id" ref="rule_cat_ING" />
        </record>

        <record id="rule_cat_DESC" model="hr.salary.rule.category">
            <field name="code">DESC</field>
            <field name="name">Descuentos</field>
            <field name="parent_id" ref="rule_cat_EGRE" />
        </record>

        <record id="rule_cat_Benef" model="hr.salary.rule.category">
            <field name="code">BENEF</field>
            <field name="name">Beneficio</field>
        </record>
    </data>
</odoo>
